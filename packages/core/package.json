{
  "name": "vue-excalidraw",
  "version": "0.1.0",
  "description": "Vue 3 component wrapping Excalidraw's React whiteboard editor",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs"
    },
    "./src/*": "./src/*"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^5.1.4"
  },
  "peerDependencies": {
    "@excalidraw/excalidraw": "^0.18.0",
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@excalidraw/excalidraw": "^0.18.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "@types/node": "^22.0.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "typescript": "^5.5.0",
    "unbuild": "^3.0.0",
    "vue": "^3.5.0",
    "vue-tsc": "^3.2.4"
  },
  "keywords": [
    "vue",
    "vue3",
    "excalidraw",
    "whiteboard",
    "drawing",
    "canvas",
    "react-vue-bridge",
    "component"
  ],
  "author": "alsey89",
  "homepage": "https://github.com/alsey89/vue-excalidraw",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alsey89/vue-excalidraw.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/alsey89/vue-excalidraw/issues"
  }
}
